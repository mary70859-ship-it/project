<?xml version="1.0" encoding="utf-8"?>
<data-extraction-rules>
    <cloud-backup>
        <!-- Exclude sensitive data from cloud backup -->
        <exclude domain="database" path="ar_education_db"/>
        <exclude domain="sharedpref" path="user_preferences.xml"/>
        <exclude domain="file" path="user_data/"/>
    </cloud-backup>
    <device-transfer>
        <!-- Exclude sensitive data from device transfer -->
        <exclude domain="database" path="ar_education_db"/>
        <exclude domain="sharedpref" path="user_preferences.xml"/>
    </device-transfer>
</data-extraction-rules>